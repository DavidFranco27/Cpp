#include <stdio.h>
#include<stdlib.h>
#include<string.h>
#define NPERSONAS 3

int main(void) {
	int cont;
	char *lista[NPERSONAS];
	char *pintercambio;
	struct lista{
		int dia;
		int mes;
		int anio;
		char nombre[15]
	};
	struct lista p[NPERSONAS];
	for(cont=0, cont<NPERSONAS,cont++){
		printf("\nDime el dia: ");
		scanf("%d",p(cont).dia);
		printf("\nDime el mes: ");
		scanf("%d",p(cont).mes);
		printf("\nDime el anio: ");
		scanf("%d",p(cont).anio);
		printf("\nDime el nombre: ");
		scanf("%s",p(cont).nombre);
	}
	printf();
	return 0;
}
