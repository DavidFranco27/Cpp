#include<stdio.h>
//Intercambio a tres bandas
void intercambia(int a,int b){
	int aux;
	aux=a;
	a=b;
	b=aux;
}

int main(){
	int a; //David
	int b; //Antonio
	int aux; //Jorge
	a=2;
	b=3;
	printf("\nANTES DEL INTERCAMBIO");
	printf("\na=%d",a);
	printf("\nb=%d",b);
	//Intercambio
	intercambia(a,b);
	printf("\nDESPUES DEL INTERCAMBIO");
	printf("\na=%d",a);
	printf("\nb=%d",b);
	return 0;

}
