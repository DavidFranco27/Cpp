#include<stdio.h>
//Programa de inventario
int main(){
	struct Articulo{
		int cod;
		float precio;
		char existe;
	};
	struct Articulo m1;
	m1.cod=1;
	m1.precio=1.30;
	m1.existe='s';
	printf("\nArticulos:");
	printf("\nCODIGO\tPRECIO\tEXISTENCIA");
	printf("\n%d\t%.2f\t%c",m1.cod,m1.precio,m1.existe);
	return 0;
}
