#include<stdio.h>
//Preguntamos los nombres de los tres reyes magos
int main(){
	//String lista[3] como en python para hacerlo
	char nombre1[9];
	char nombre2[9];
	char nombre3[9];
	printf("\nDi el nombre del primer rey mago: ");
	scanf("%s",nombre1);
	printf("\nDi el nombre del segundo rey mago: ");
	scanf("%s",nombre2);
	printf("\nDi el nombre del tercer rey mago: ");
	scanf("%s",nombre3);
	//Los sacamos por pantalla
	printf("\n1ยบ %s",nombre1);
	printf("\n2ยบ %s",nombre2);
	printf("\n3ยบ %s",nombre3);
	
	return 0;
}
